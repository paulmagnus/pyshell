import os

for f in $ls$:
    if (f != 'run_tests.pysh' and f[-5:] == '.pysh'):
        print('##################################################')
        print(f)
        print('>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>')
        $../bin/pyshell @f$
        print('##################################################', end='\n\n')
print('All tests complete!')
